<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
<bean id="question" class="com.ass9.BeanLifeCycle" init-method="myInit" destroy-method="destroy">
<property name="questionId" value="1" />
<property name="question" value="What are your hobbies ?" />
<property name="answer">
<list>
      <value>playing football</value>
      <value>reading book</value>
</list>
</property>

<property name="ans">
<set>
      <value>sleeping</value>
      <value>singing</value>
</set>
</property>

<property name="a">
<map>
      <entry key="1"><value>eating</value></entry>
      <entry key="2"><value>dancing</value></entry>
</map>
</property>

</bean>
</beans>